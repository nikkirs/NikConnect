<% include partials/header %>
	<div class="container" style="width:90%;margin:40px auto">
		<h2 style="text-align:center">all users</h2>
		<div class="row text-center" style="display:flex;flex-wrap:wrap">
			<%a.forEach(function(d){%>

				<div class="col-lg-12 col-sm-12">
					<div class="thumbnail">
						<div class="caption">
							<h1>
								<%=d.email%>
							</h1>
							<a class="btn btn-primary" href="/photos/<%=d.user_id%>" style="width: 100px;margin-bottom: 15px">photos</a>
							</br>
							<a class="btn btn-danger" href="/follow/<%=d.user_id%>" style="width: 100px">Follow</a>
						</div>
					</div>
				</div>


				<%})%>
		</div>
	</div>

	<% include partials/footer %>